            android:name="com.example.newapplication.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.newapplication.Example"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.newapplication.EXAMPLE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.newapplication.MyMenu" >
            <intent-filter>
                <action android:name="com.example.newapplication.MENU" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.newapplication.Prefs"
            android:label="@string/app_name" >
